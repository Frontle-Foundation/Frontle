  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".frontle": true,
    "typings": true,
    "jsconfig.json": true
  },
  "path-autocomplete.pathMappings": {
    "@": "${folder}/www/version/@"
